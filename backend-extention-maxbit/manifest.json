            "https://admin.crimson.volna.prd.maxbit.private/*",
            "https://admin.crimson.legzo.prd.maxbit.private/*",
            "https://admin.crimson.starda.prd.maxbit.private/*",
            "https://admin.crimson.drip.prd.maxbit.private/*",
            "https://admin.crimson.monro.prd.maxbit.private/*",
			"https://admin.crimson.lex.prd.maxbit.private/*",
			  "https://admin.crimson.irwin.prd.maxbit.private/*",
            "https://supdeskt.atlassian.net/servicedesk/customer/*",
